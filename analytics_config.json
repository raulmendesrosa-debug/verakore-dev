      "exit_pages": true
    }
  },
  "tracking": {
    "google_analytics": {
      "enabled": false,
      "tracking_id": "",
      "measurement_id": ""
    },
    "custom_analytics": {
      "enabled": true,
      "privacy_compliant": true,
      "cookie_consent": true
    }
  },
  "reports": {
    "daily_summary": true,
    "weekly_report": true,
    "monthly_analysis": true,
