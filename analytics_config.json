{
  "analytics": {
    "enabled": true,
    "collection_interval_minutes": 15,
    "retention_days": 365,
    "real_time_enabled": true
  },
  "data_sources": {
    "website_analytics": {
      "enabled": true,
      "page_views": true,
      "user_sessions": true,
      "bounce_rate": true,
      "conversion_tracking": true
    },
    "performance_metrics": {
      "enabled": true,
